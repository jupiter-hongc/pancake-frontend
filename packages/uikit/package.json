{
  "name": "@pancakeswap/uikit",
  "version": "0.67.1",
  "description": "Set of UI components for pancake projects",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "*.css.ts",
    "src/css/**/*",
    "src/theme/**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./css/atoms": {
      "import": "./src/css/atoms.ts",
      "types": "./dist/css/atoms.d.ts"
    },
    "./css/vars.css": {
      "import": "./src/css/vars.css.ts",
      "types": "./dist/css/vars.css.d.ts"
    },
    "./css/responsiveStyle": {
      "import": "./src/css/responsiveStyle.ts",
      "types": "./dist/css/responsiveStyle.d.ts"
    }
  },
  "repository": "https://github.com/pancakeswap/pancake-frontend/tree/develop/packages/uikit",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:uikit": "vite build",
    "dev": "vite build --watch --mode development",
    "start": "pnpm storybook",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format:check": "prettier --check --loglevel error 'src/**/*.{js,jsx,ts,tsx}'",
    "format:write": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "test": "vitest --run",
    "update:snapshot": "vitest -u",
    "prepublishOnly": "pnpm run build:uikit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@babel/core": "catalog:",
    "@babel/preset-env": "catalog:",
    "@babel/preset-react": "catalog:",
    "@pancakeswap/chains": "workspace:*",
    "@pancakeswap/hooks": "workspace:*",
    "@pancakeswap/utils": "workspace:*",
    "@storybook/addon-a11y": "catalog:",
    "@storybook/addon-actions": "catalog:",
    "@storybook/addon-essentials": "catalog:",
    "@storybook/addon-links": "catalog:",
    "@storybook/builder-vite": "catalog:",
    "@storybook/react": "catalog:",
    "@storybook/react-vite": "catalog:",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^12.1.3",
    "@types/d3": "catalog:",
    "@types/js-cookie": "catalog:",
    "@types/lodash": "^4.14.168",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/react-router-dom": "catalog:",
    "@types/react-transition-group": "catalog:",
    "@types/styled-system__should-forward-prop": "catalog:",
    "@vanilla-extract/vite-plugin": "^3.8.0",
    "@vitejs/plugin-react": "catalog:",
    "babel-loader": "catalog:",
    "babel-plugin-styled-components": "catalog:",
    "d3": "catalog:",
    "happy-dom": "catalog:",
    "jest-styled-components": "catalog:",
    "js-cookie": "catalog:",
    "next-themes": "catalog:",
    "polished": "catalog:",
    "react": "catalog:",
    "react-countup": "catalog:",
    "react-device-detect": "*",
    "react-dom": "catalog:",
    "react-is": "catalog:",
    "react-markdown": "catalog:",
    "react-redux": "catalog:",
    "react-router-dom": "catalog:",
    "react-transition-group": "*",
    "remark-gfm": "*",
    "rollup-plugin-node-builtins": "catalog:",
    "storybook": "catalog:",
    "styled-components": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vite-tsconfig-paths": "catalog:"
  },
  "peerDependencies": {
    "js-cookie": "^3.0.5",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-device-detect": "*",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "react-transition-group": "*",
    "remark-gfm": "*",
    "styled-components": "6.0.7"
  },
  "dependencies": {
    "@pancakeswap/hooks": "workspace:*",
    "@pancakeswap/localization": "workspace:*",
    "@popperjs/core": "catalog:",
    "@radix-ui/react-dismissable-layer": "catalog:",
    "@radix-ui/react-slot": "catalog:",
    "@styled-system/should-forward-prop": "catalog:",
    "@types/styled-system": "catalog:",
    "@vanilla-extract/css": "catalog:",
    "@vanilla-extract/css-utils": "catalog:",
    "@vanilla-extract/recipes": "catalog:",
    "@vanilla-extract/sprinkles": "catalog:",
    "bignumber.js": "^9.0.0",
    "clsx": "catalog:",
    "csstype": "catalog:",
    "dayjs": "catalog:",
    "deepmerge": "catalog:",
    "framer-motion": "catalog:",
    "lightweight-charts": "catalog:",
    "lodash": "^4.17.20",
    "primereact": "catalog:",
    "react-popper": "catalog:",
    "sonner": "catalog:",
    "styled-system": "catalog:",
    "tslib": "^2.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
